{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es2017",
        "moduleResolution": "Node",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        // "jsx": "react",
        "esModuleInterop": true
    },
    "include": ["./*.ts"]
}
